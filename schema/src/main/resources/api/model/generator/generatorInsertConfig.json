{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "properties" : {
    "id" : {
      "type" : "string",
      "nullable" : false
    },
    "valueType" : {
      "type" : "string",
      "enum" : [ "integer", "double", "string" ],
      "nullable" : false
    },
    "keyValue" : {
      "type" : "string",
      "nullable" : true
    },
    "maxKeyLength" : {
      "type" : "integer",
      "nullable" : true
    },
    "minKeyLength" : {
      "type" : "integer",
      "nullable" : true
    },
    "individualKey" : {
      "type" : "boolean",
      "default": false,
      "nullable" : true
    },
    "maxStringEnumValues" : {
      "type" : "integer",
      "nullable" : true
    },
    "minStringEnumValues" : {
      "type" : "integer",
      "nullable" : true
    },
    "stringEnumValues" : {
      "type" : "array",
      "nullable" : true,
      "items" : {
        "type" : "string"
      },
      "minItems": 1
    },
    "maxStringValueLength" : {
      "type" : "integer",
      "nullable" : true
    },
    "minStringValueLength" : {
      "type" : "integer",
      "nullable" : true
    },
    "maxValue" : {
      "type" : "number",
      "nullable" : true
    },
    "minValue" : {
      "type" : "number",
      "nullable" : true
    },
    "gaussMiddle" : {
      "type" : "number",
      "nullable" : true
    },
    "gaussRange" : {
      "type" : "number",
      "nullable" : true
    },
    "triangleSpike" : {
      "type" : "number",
      "nullable" : true
    },
    "valueDistribution" : {
      "type" : "string",
      "enum" : [ "UNIFORM", "GAUSS", "TRIANGLE" ],
      "nullable" : true
    }
  },
  "required": [ "id","valueType" ]
}