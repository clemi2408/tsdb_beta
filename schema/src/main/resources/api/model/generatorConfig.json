{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "properties" : {
    "recordConfigs" : {
      "type" : "array",
      "items" : {
        "$ref": "recordConfig.json"
      },
      "minItems": 1
    },
    "recordCount" : {
      "type" : "integer",
      "minimum": 1
    }
  },
  "required": [ "recordCount","recordConfigs" ]
}