{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "properties" : {
    "kvPairs" : {
      "type" : "array",
      "nullable" : false,
      "items" : {
      "$ref": "kvPair.json"
      },
      "minItems": 1
    },
    "id" : {
      "type" : "string",
      "format": "uuid",
      "nullable" : false
    },
    "timestamp": {
      "type": "string",
      "format": "date-time",
      "nullable": true
    }
  },
  "required": [ "id","kvPairs" ]

}