{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "properties" : {

    "orchestratorUrl" : {
      "type" : "string",
      "nullable" : true,
      "format": "uri",
      "pattern": "^https?://"
    },

    "workerGeneralProperties": {
      "$ref": "workerGeneralProperties.json",
      "nullable" : false
    },
    "workerConfiguration": {
      "$ref": "workerConfiguration.json",
      "nullable" : false
    },
    "benchmarkWorkload" : {
      "$ref": "../workload/workload.json",
      "nullable" : false
    },
    "workerLoadCount" : {
      "type" : "integer",
      "minimum": 0,
      "nullable" : false
    }
  },
  "required": [ "workerLoadCount","orchestratorUrl","workerGeneralProperties","workerConfiguration","benchmarkWorkload" ]
}